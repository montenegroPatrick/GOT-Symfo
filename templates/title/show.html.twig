{% extends 'base.html.twig' %}

{% block title %}Title{% endblock %}

{% block body %}
    <h1>Title</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ title.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ title.createdAt ? title.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ title.updatedAt ? title.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ title.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_title_index') }}">back to list</a>

    <a href="{{ path('app_title_edit', {'id': title.id}) }}">edit</a>

    {{ include('title/_delete_form.html.twig') }}
{% endblock %}
